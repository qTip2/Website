!!! 5
html
	head
		title qTip2 - Pretty powerful tooltips #{locals.title ? ' - ' + title.join(' - ') : ''}

		// Metadata
		include includes/metadata

		// Stylesheets
		link(rel='shortcut icon', type='image/vnd.microsoft.icon', href='#{url("/favicon.ico")}')
		link(rel="stylesheet", type='text/css', href="#{url('/stylesheets/reset.css')}")
		link(rel="stylesheet", type='text/css', href="#{url('/stylesheets/new.css')}")
		link(rel="stylesheet", type='text/css', href="#{url('/stylesheets/demos.css')}")
		link(rel="stylesheet", type='text/css', href="#{url('/stylesheets/hightlight2.css')}")
		link(rel='stylesheet', type='text/css', href='#{qtip("nightly", "css")}')
		link(rel='stylesheet', type='text/css', href='http://fonts.googleapis.com/css?family=Average+Sans|Montserrat|Open+Sans:400,300,600,700|Droid+Sans+Mono&subset=latin,latin-ext')
		block stylesheets

		// jQuery
		script(type='text/javascript', src='http://code.jquery.com/jquery.min.js')

		// Google Analytics
		include includes/analytics

	mixin featureblock(text, classes)
		span(class=classes) !{text}

	mixin demoblock(text, fiddle)
		span !{text}
		- if(fiddle)
			mixin sourcelink(fiddle)

	mixin sourcelink(fiddle)
		a.source(href='http://jsfiddle.net/craga89/'+fiddle, target="_blank") view source

	mixin logo()
		strong qTip<sup>2</sup>&nbsp;

