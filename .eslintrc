{
    "env": {
        "node": true
    },
    "rules": {
        "strict": [
            2,
            "never"
        ],
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "new-cap": [
            2,
            {
                capIsNewExceptions: ["Q"]
            }
        ]
    }
}